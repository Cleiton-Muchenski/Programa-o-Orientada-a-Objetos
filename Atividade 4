------------------------------------------------------------------------------------------------------------------------------------- AT4 - Q5
public class Main
{
	public static void main(String[] args) {
    int i;

    for (i = 1;i <= 20; i++) {
        if (i % 2 == 1) {
            System.out.println("Numero: " + i + " é impar!");
        }
    }
    
	}
}
------------------------------------------------------------------------------------------------------------------------------------- AT4 - Q6
public class Main
{
	public static void main(String[] args) {
    int i;

    for (i = 1;i <= 200; i++) {
        if (i % 4 == 0) {
            System.out.println("Numero: " + i + " é divisivel por 4!");
        }
    }
    
	}
}
