------------------------------------------------------------------------------------------------------------------------------------------- Atividade 3 - Q1
import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	    int numero;
	    Scanner prompt = new Scanner(System.in);
	    
	    numero = prompt.nextInt();
	    
	    if (!(numero > 3)){
	        System.out.println("O numero: " + numero + " não é maior que 3");
	    }
	    
	}
}
